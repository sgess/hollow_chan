 RowBox[{
  RowBox[{"Needs", "[", "\"\<PlotLegends`\>\"", "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Plot", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Piecewise", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"0", ",", 
          RowBox[{"r", "<", 
           SubscriptBox["R", "s"]}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"-", "2"}], "/", "r"}], ",", 
          RowBox[{
           RowBox[{"r", ">", 
            SubscriptBox["R", "s"]}], " ", "&&", " ", 
           RowBox[{"r", " ", "<", " ", "1"}]}]}], "}"}]}], "}"}], "]"}], ",", 
     
     RowBox[{"Piecewise", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"2", "\[Lambda]", "*", 
           RowBox[{"Log", "[", 
            SubscriptBox["R", "s"], "]"}]}], ",", 
          RowBox[{"r", "<", 
           SubscriptBox["R", "s"]}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"2", "\[Lambda]", "*", 
           RowBox[{"Log", "[", "r", "]"}]}], ",", 
          RowBox[{
           RowBox[{"r", ">", 
            SubscriptBox["R", "s"]}], " ", "&&", " ", 
           RowBox[{"r", " ", "<", " ", "1"}]}]}], "}"}]}], "}"}], "]"}]}], 
    "}"}], ",", 
   RowBox[{"{", 
